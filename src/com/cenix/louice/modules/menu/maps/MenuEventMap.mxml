                          imageUrl="assets/icons/menu/organizations.png"
                          contentType="{ContentType.ORGANIZATION_COLLECTION}" />


            <vos:MenuItem uid="17"
                          url="/stock-info/"
                          title="Stock Info"
                          description="Stock related information"
                          imageUrl="assets/icons/menu/stock-info.png"
                          prefetch="false"

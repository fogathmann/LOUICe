            <vos:MenuItem uid="21"
                          url="{'/lab-iso-requests/?q=owner:contains:' + getIsoOwnerFilter()}"
                          title="ISO Requests"
                          description="Manage ISO Requests"
                          imageUrl="assets/icons/menu/iso-requests.png"
                          prefetch="true"
                          contentType="{ContentType.ISO_REQUEST_COLLECTION}" />

            <vos:MenuItem uid="22"
                          url="/barcodes/"
                          title="Label Print"

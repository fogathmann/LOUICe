            <vos:MenuItem uid="18"
                          url="/devices/"
                          title="Devices"
                          description="Manage devices"
                          imageUrl="assets/icons/menu/devices.png"
                          contentType="{ContentType.DEVICE_COLLECTION}" />

			<vos:MenuItem uid="19"

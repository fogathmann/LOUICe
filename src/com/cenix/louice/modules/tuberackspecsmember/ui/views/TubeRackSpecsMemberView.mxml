<?xml version="1.0" encoding="utf-8"?>
<views1:MemberView xmlns:fx="http://ns.adobe.com/mxml/2009"
                 xmlns:s="library://ns.adobe.com/flex/spark"
                 xmlns:mx="library://ns.adobe.com/flex/mx"
                 xmlns:components="com.cenix.louice.shared.ui.components.*"
                 xmlns:views="com.cenix.louice.modules.devicemember.ui.views.*"
                xmlns:view="org.everest.flex.ui.views.*"
                 width="100%" height="100%" xmlns:components1="org.everest.flex.ui.components.*" xmlns:views1="org.everest.flex.ui.views.*">

    <s:Label text="{'Tube Rack Specs: ' + model.title}" fontWeight="bold"
             fontSize="14" />

    <s:Form width="100%">
        <s:FormItem label="Label" width="100%">
            <s:Label text="{model.label}" />
        </s:FormItem>
        <s:FormItem label="Shape" width="100%">
            <components1:MemberLinkButton link="{model.shape}" />
        </s:FormItem>
        <s:FormItem label="Manufacturer" width="100%">
            <components1:MemberLinkButton link="{model.manufacturer}" />
        </s:FormItem>
        <s:FormItem label="Tube Specs" width="100%">
            <components1:MemberLinkButton link="{model.tube_specs}" />
        </s:FormItem>
    </s:Form>
</views1:MemberView>
